<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="nav">
        <item name="colorPrimary">@color/teal_700</item>
        <item name="colorControlHighlight">@color/teal_700</item>
    </style>
    <style name="AppTheme.StandardButtonSecondary" parent="AppTheme.StandardButton">
        <item name="android:background">@drawable/button_border_secondary</item>
        <item name="android:textColor">@color/primary6</item>
    </style>
    <style name="AppTheme.StandardButton" parent="android:Widget.Button">
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/button_main</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/primary</item>
    </style>
    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/bg_spinner</item>
    </style>
</resources>